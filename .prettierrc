{
	"semi": false,
	"trailingComma": "es5",
	"printWidth": 120,
	"useTabs": false,
	"tabWidth": 2,
	"singleQuote": true,
	"quoteProps": "consistent",
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "auto",
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
